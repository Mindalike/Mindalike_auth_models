{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12"
    },
    "commands": {
      "build": "pip install -r requirements.txt"
    }
  },
  "public": true
}