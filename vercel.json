{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/predict",
      "dest": "main.py",
      "methods": ["POST"],
      "continue": true
    },
    {
      "src": "/",
      "dest": "main.py",
      "methods": ["GET"],
      "continue": true
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12"
    },
    "commands": {
      "build": "pip install -r requirements.txt"
    }
  },
  "public": true
}